mf_labeller <- function(var, value){
value <- as.character(value)
if (var=="strat") {
value[value=="30"] <- "30 days"
value[value=="60"]   <- "60 days"
}
if (var=="temp") {
value[value=="11.3"] <- "11.3 째C"
value[value=="16"]   <- "16 째C"
value[value=="20.7"]   <- "20.7 째C"
value[value=="25.3"]   <- "25.3 째C"
}
return(value)
}
